{
  "name": "rollup-plugin-yamlx",
  "version": "0.0.1",
  "description": "Enhanced yaml plugin for rollup/vite.",
  "keywords": [
    "rollup-plugin",
    "vite-plugin",
    "yaml"
  ],
  "author": "unbyte <i@shangyes.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unbyte/rollup-plugin-yamlx"
  },
  "homepage": "https://github.com/unbyte/rollup-plugin-yamlx",
  "bugs": {
    "url": "https://github.com/unbyte/rollup-plugin-yamlx/issues/new"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node scripts/build.mjs",
    "lint": "eslint . --cache",
    "lint:nocache": "eslint .",
    "test": "jest",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "js-yaml": "^4.1.0",
    "tosource": "^2.0.0-alpha.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/js-yaml": "^4.0.2",
    "@types/node": "^16.4.13",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "is-ci": "^3.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "rollup": "^2.56.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.4",
    "tsup": "^4.13.1",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "eslintIgnore": [
    "dist",
    "test",
    "jest.config.js"
  ],
  "publishConfig": {
    "access": "public"
  }
}
